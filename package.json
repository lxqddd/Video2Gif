{
  "name": "video2gif",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "视频转 gif 的桌面应用，支持屏幕录制",
  "author": "向阳",
  "license": "MIT",
  "keywords": [
    "video",
    "gif",
    "Video",
    "Gif",
    "screen recording"
  ],
  "main": "dist-electron/main/index.js",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/fs-extra": "^11.0.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^29.1.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "sass": "^1.77.2",
    "typescript": "^5.4.2",
    "unocss": "^0.60.3",
    "vite": "^5.1.5",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.2.0",
    "sharp": "^0.33.4"
  }
}
